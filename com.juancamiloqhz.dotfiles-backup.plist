<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.juancamiloqhz.dotfiles-backup</string>

  <key>ProgramArguments</key>
  <array>
    <string>/bin/bash</string>
    <string>-l</string>
    <string>/Users/juancamiloqhz/dotfiles/scripts/auto-backup.sh</string>
  </array>

  <key>StartCalendarInterval</key>
  <dict>
    <key>Hour</key>
    <integer>12</integer>
    <key>Minute</key>
    <integer>0</integer>
  </dict>

  <key>StandardOutPath</key>
  <string>/Users/juancamiloqhz/.local/share/dotfiles-backup/backup.log</string>

  <key>StandardErrorPath</key>
  <string>/Users/juancamiloqhz/.local/share/dotfiles-backup/backup.log</string>
</dict>
</plist>
